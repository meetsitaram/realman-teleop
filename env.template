# RealMan Robot Configuration Template
# Copy this file to .env and fill in your robot's details
# This file will be automatically loaded by all scripts

# Robot Connection Settings
ROBOT_IP=192.168.10.18
ROBOT_PORT=8080
ROBOT_MODEL=R1D2

# Robot Configuration
# DOF (Degrees of Freedom) - Leave empty for auto-detection
# Set to 6 or 7 if you want to override auto-detection
# ROBOT_DOF=

# Control Settings
DEFAULT_SPEED=0.1
DEFAULT_UPDATE_RATE=100

# Logging
LOG_LEVEL=INFO

